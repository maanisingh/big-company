{
  "name": "bigcompany-medusa-backend",
  "version": "1.0.0",
  "description": "BIG Company E-commerce Backend powered by Medusa",
  "main": "index.js",
  "scripts": {
    "dev": "medusa develop",
    "start": "npm run init:bigcompany && npm run migrations:run && NODE_OPTIONS='--enable-source-maps --trace-warnings --unhandled-rejections=strict' medusa start 2>&1",
    "start:prod": "medusa start",
    "build": "npm run build:admin",
    "build:admin": "medusa-admin build --deployment",
    "seed": "medusa seed -f ./data/seed.json",
    "migrations:run": "medusa migrations run",
    "migrations:generate": "medusa migrations generate",
    "init:bigcompany": "node scripts/init-bigcompany-schema.js"
  },
  "dependencies": {
    "pg": "^8.11.0",
    "@medusajs/admin": "^7.1.0",
    "@medusajs/cache-inmemory": "^1.8.0",
    "@medusajs/cache-redis": "^1.9.0",
    "@medusajs/event-bus-local": "^1.9.0",
    "@medusajs/event-bus-redis": "^1.8.0",
    "@medusajs/file-local": "^1.0.0",
    "@medusajs/medusa": "^1.20.0",
    "@tanstack/react-query": "^4.22.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.2",
    "medusa-fulfillment-manual": "^1.1.0",
    "medusa-interfaces": "^1.3.0",
    "medusa-payment-manual": "^1.0.0",
    "typeorm": "^0.3.0",
    "africastalking": "^0.6.0",
    "bullmq": "^5.1.0",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/medusa-cli": "^1.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.13",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
